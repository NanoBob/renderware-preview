<UserControl x:Class="RenderWarePreviewer.Ui.Controls.WeaponModelPicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RenderWarePreviewer.Ui.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="200">
    <StackPanel Name="WeaponStackPanel">
        <Label Foreground="white">Weapon:</Label>
        <ComboBox Name="WeaponComboBox" Margin="0, 0, 5, 0" SelectionChanged="SelectModel">
            <ComboBoxItem>AK 47</ComboBoxItem>
        </ComboBox>
    </StackPanel>
</UserControl>
